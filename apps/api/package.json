{
  "name": "alice-medical-backend-",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:stats": "eslint \"{src,apps,libs,test}/**/*.ts\" --format ./node_modules/eslint-stats/byErrorAndWarningStacked.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "DOTENV_CONFIG_PATH=./e2e-resources/.env.e2e jest --verbose --silent --runInBand --config ./test/jest-e2e.json --setupFiles dotenv/config",
    "prepare": "husky install"
  },
  "dependencies": {
    "@azure/msal-node": "^2.8.1",
    "@nestjs-modules/mailer": "^1.10.3",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^10.0.1",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
    "@vercel/node": "^3.2.9",
    "api": "^6.1.2",
    "aws-cloudfront-sign": "^3.0.2",
    "aws-sdk": "^2.1569.0",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csv-writer": "^1.6.0",
    "google-auth-library": "^9.6.0",
    "jwks-rsa": "^3.1.0",
    "moment": "^2.30.1",
    "nest-winston": "^1.9.4",
    "nestjs-cls": "^4.0.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rrule": "^2.8.1",
    "rxjs": "^7.2.0",
    "speakeasy": "^2.0.0",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "unzipper": "^0.12.3",
    "winston": "^3.11.0",
    "winston-slack-webhook-transport": "^2.3.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/lodash": "^4.17.0",
    "@types/node": "^16.0.0",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "jest": "^28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.4",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "verbose": true,
    "bail": true
  }
}
